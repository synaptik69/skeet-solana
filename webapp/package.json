  "private": false,
  "scripts": {
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext .ts,.tsx --fix .",
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "typecheck": "tsc --noEmit",
    "update:packages": "ncu -u -x 'node-fetch,glob' && yarn",
    "dev": "next dev -p 4200",
